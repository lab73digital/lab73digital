  log('widgetsCfg');
  log(widgetsCfg);
  log('extConfig');
  log(extConfig);

  var targetUrl = 'http://www.likar.info/likardisklamer/index.html';
  getTargetUrl();

  debugLog(targetUrl, "url");

  var seed = 'xJkOWl0BPndNYxFqqKI';

  onFuncReady({
    funcName: 'relapHeadjsLoaded',
    args: [{
      seed: seed,
