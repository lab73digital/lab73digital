function adsGoogle() {

    googletag.cmd.push(function() {
        googletag.defineSlot('/67342444/likar_300x250', [[336, 280], [300, 250]], 'div-gpt-ad-1493021295710-0').addService(googletag.pubads());
        googletag.defineSlot('/67342444/likar_300x250', [[336, 280], [300, 250]], 'ca-pub-2794222139507009').addService(googletag.pubads());
        googletag.defineSlot('/67342444/likar_300x250', [[336, 280], [300, 250]], 'ca-pub-2794222139507009a').addService(googletag.pubads());
        
        googletag.defineSlot('/67342444/likar_300x250', [[336, 280], [300, 250]], 'div-gpt-ad-1468330504524-0').addService(googletag.pubads());
        googletag.defineSlot('/67342444/likar_300x250_2', [[336, 280], [300, 250]], 'div-gpt-ad-1468330504524-1').addService(googletag.pubads());
        googletag.defineSlot('/67342444/likar_300x250_3', [[336, 280], [300, 250]], 'div-gpt-ad-1468330504524-2').addService(googletag.pubads());

        googletag.defineSlot('/67342444/likar_300*250_section', [300, 250], 'div-gpt-ad-1468484818548-0').addService(googletag.pubads());
        googletag.defineSlot('/67342444/likar_300*250_section', [300, 250], 'div-gpt-ad-1468484818548-1').addService(googletag.pubads());
        googletag.defineSlot('/67342444/likar_300*250_section', [300, 250], 'div-gpt-ad-1468484818548-2').addService(googletag.pubads());
        googletag.defineSlot('/67342444/likar_300*250_section', [300, 250], 'div-gpt-ad-1468484818548-3').addService(googletag.pubads());

        googletag.pubads().enableSingleRequest();
        googletag.enableServices();
    });

    googletag.cmd.push(function() { googletag.display('div-gpt-ad-1493021295710-0'); });
    googletag.cmd.push(function() { googletag.display('ca-pub-2794222139507009'); });
    googletag.cmd.push(function() { googletag.display('ca-pub-2794222139507009a'); });

    googletag.cmd.push(function() { googletag.display('div-gpt-ad-1468330504524-0'); });
    googletag.cmd.push(function() { googletag.display('div-gpt-ad-1468330504524-1'); });
    googletag.cmd.push(function() { googletag.display('div-gpt-ad-1468330504524-2'); });

    googletag.cmd.push(function() { googletag.display('div-gpt-ad-1468484818548-0'); });
    googletag.cmd.push(function() { googletag.display('div-gpt-ad-1468484818548-1'); });
    googletag.cmd.push(function() { googletag.display('div-gpt-ad-1468484818548-2'); });
    googletag.cmd.push(function() { googletag.display('div-gpt-ad-1468484818548-3'); });

    for(var i = 0; i < $('.adsbygoogle').length; i++) {

        //$('.adsbygoogle')[i].removeAttribute('id');
        (adsbygoogle = window.adsbygoogle || []).push({});
    }
}

