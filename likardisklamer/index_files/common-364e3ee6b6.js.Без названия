var proteflazid = function ($) {

    var sct, hex_ot, c_ot, shown1=false, shown2=false, shown3=false, caution__img, now_scrolling=false, sticked=false;

    function parallax(elem) {
        var mid_x=0;
        var mid_y=0;
        this.recalc = function() {
            this.mid_x = $(elem).width()/2;
            this.mid_y = $(elem).height()/2;
        }

        this.moveTr = function (e) {
            var posX = $(elem).offset().left, posY = $(elem).offset().top;
            var mx=e.pageX - posX;
            var my=e.pageY - posY;
            var bx = 30*(mx-this.mid_x)/this.mid_x;
            var by = 30*(my-this.mid_y)/this.mid_y;
            $(elem).css('background-position','calc(50% + '+Math.floor(bx).toString()+'px)'+' calc(50% + '+Math.floor(by).toString()+'px)');
        }
    }


    $(function () {

        $('#gazetaConfirm').show();
        $('.close-popup').on('click', function () {
            $('#gazetaConfirm').hide();
        });

        prlx = new parallax('.bg');
        prlx.recalc();

        $('.bg').mousemove(function (e) {
            prlx.moveTr(e);
        });




        prlx2 = new parallax('.bg3');
        prlx2.recalc();

        $('.bg3').mousemove(function (e) {
            prlx2.moveTr(e);
        });

        prlx3 = new parallax('.bg4');
        prlx3.recalc();

        $('.bg4').mousemove(function (e) {
            prlx3.moveTr(e);
        });

        hex_ot = $('.hex-1').offset().top-0.8*$(window).height();
        c_ot = $('.c1').offset().top-0.8*$(window).height();
        caution__img = $('.caution__item-img-1').offset().top-0.8*$(window).height();
    });

    $(window).resize(function () {
      hex_ot = $('.hex-1').offset().top-0.8*$(window).height();
      c_ot = $('.c1').offset().top-0.8*$(window).height();
      caution__img = $('.caution__item-img-1').offset().top-0.8*$(window).height();
  });

    $(window).scroll(function () {
        sct = $(window).scrollTop();
        if ((sct>hex_ot) && (!shown1)) {

            setTimeout(function () {
                $('.hex-1').addClass('zoomIn');
                $('.hex-1').css('visibility','visible');
                setTimeout(function () {
                    $('.hex-2').addClass('zoomIn');
                    $('.hex-2').css('visibility','visible');
                    setTimeout(function () {
                        $('.hex-3').addClass('zoomIn');
                        $('.hex-3').css('visibility','visible');
                        setTimeout(function () {
                            $('.hex-4').addClass('zoomIn');
                            $('.hex-4').css('visibility','visible');
                        },300)
                    },300);
                }, 300);
            }, 300);

            shown1=true;
        }

        if ((sct>c_ot) && (!shown2)) {

            setTimeout(function () {
                $('.c1').addClass('zoomIn');
                $('.c1').css('visibility','visible');
                setTimeout(function () {
                    $('.c2').addClass('zoomIn');
                    $('.c2').css('visibility','visible');
                    setTimeout(function () {
                        $('.c3').addClass('zoomIn');
                        $('.c3').css('visibility','visible');
                        setTimeout(function () {
                            $('.mark').addClass('zoomIn');
                            $('.mark').css('visibility','visible');
                        }, 300);
                    }, 300);
                }, 300)
            }, 300);

            shown2=true;
        }

        if ((sct>caution__img) && (!shown3)) {

            setTimeout(function () {
                $('.caution__item-img-1').addClass('zoomIn');
                $('.caution__item-img-1').css('visibility','visible');
                setTimeout(function () {
                    $('.caution__item-img-2').addClass('zoomIn');
                    $('.caution__item-img-2').css('visibility','visible');
                    setTimeout(function () {
                        $('.caution__item-img-3').addClass('zoomIn');
                        $('.caution__item-img-3').css('visibility','visible');
                    }, 300);
                }, 300);
            }, 300);

            shown3=true;
        }


        if ($(".top-menu-fixed").length>0) {

            if (!sticked) sticky_top = $(".top-menu-fixed").offset().top;

            if (sct>=sticky_top) {
                $(".top-menu-fixed").css({
                    'position': 'fixed',
                    'top': '0px',
                    'left': '0px',
                    'width': '100%'
                });

                sticked=true;
            }
            else {
                $(".top-menu-fixed").css({'position': 'static'});
                sticked=false;
            }
        }

        $('.top-menu__link').removeClass('top-menu__link_active');
        if (sct+$(".top-menu-fixed").height()+200<$('#farm-sv').offset().top) {
            if (!$('a[href="#sostav"]').hasClass('top-menu__link_active'))
                $('a[href="#sostav"]').addClass('top-menu__link_active');
        } else if ((sct+$(".top-menu-fixed").height()+200>$('#farm-sv').offset().top) && 
                   (sct+$(".top-menu-fixed").height()+200<$('#osobennosti').offset().top)) {
            if (!$('a[href="#farm-sv"]').hasClass('top-menu__link_active'))
                $('a[href="#farm-sv"]').addClass('top-menu__link_active');
        } else if ((sct+$(".top-menu-fixed").height()+200>$('#osobennosti').offset().top) && 
                   (sct+$(".top-menu-fixed").height()+200<$('#info').offset().top)) {
            if (!$('a[href="#osobennosti"]').hasClass('top-menu__link_active'))
                $('a[href="#osobennosti"]').addClass('top-menu__link_active');
        } else {
            if (!$('a[href="#info"]').hasClass('top-menu__link_active'))
                $('a[href="#info"]').addClass('top-menu__link_active');            
        }

    });

    $(document).on('mousemove', '.hex', function () {
      $('.hex-1').removeClass('zoomIn');
      $('.hex-2').removeClass('zoomIn');
      $('.hex-3').removeClass('zoomIn');
      $('.hex-4').removeClass('zoomIn');
  });

    $(document).on('mousemove', '.c1,.c2,.c3', function () {
      $('.c1').removeClass('zoomIn');
      $('.c2').removeClass('zoomIn');
      $('.c3').removeClass('zoomIn');
      $('.mark').removeClass('zoomIn');
  });

    $(document).on('mousemove', '.caution__item-img-1,.caution__item-img-2,.caution__item-img-3', function () {
      $('.caution__item-img-1').removeClass('zoomIn');
      $('.caution__item-img-2').removeClass('zoomIn');
      $('.caution__item-img-3').removeClass('zoomIn');
  });    


    $(document).on('click', '.top-menu__link', function () { 
        if (now_scrolling) return false;
        now_scrolling = true;
        var elementClick = $(this).attr("href");

        var destination = $(elementClick).offset().top-$('.top-menu').height()-100;

        $('html,body').animate( { scrollTop: destination }, 1100,function () {
            now_scrolling = false;
        } );

        return false;
    });

    $(document).on('click','.triangle', function () {
        $('.spoiler').toggleClass('opened');
        $('.triangle').toggleClass('opened');
        if ($('.spoiler').hasClass('opened')) {
            $('.troetochie').hide();
            $('.full-text').css('display','block');
        } else {
            $('.troetochie').show();
            $('.full-text').hide();
        }

        return false;
    });

 
};

