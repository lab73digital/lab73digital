function marketGid() {

    var D=new Date(),d=document,b='body',ce='createElement',ac='appendChild',st='style',ds='display',n='none',gi='getElementById';
    if(!$('.lekarstva-page').length && $("#M17751ScriptRootN6082").length){
    var i=d[ce]('iframe');i[st][ds]=n;d[gi]("M17751ScriptRootN6082")[ac](i);try{var iw=i.contentWindow.document;iw.open();iw.writeln("<ht"+"ml><bo"+"dy></bo"+"dy></ht"+"ml>");iw.close();var c=iw[b];}
    catch(e){var iw=d;var c=d[gi]("M17751ScriptRootN6082");}var dv=iw[ce]('div');dv.id="MG_ID";dv[st][ds]=n;dv.innerHTML=6082;c[ac](dv);
        var s=iw[ce]('script');s.async='async';s.defer='defer';s.charset='utf-8';s.src="//jsn.marketgid.com/2/l/2.likar.info.6082.js?t="+D.getYear()+D.getMonth()+D.getDate()+D.getHours();c[ac](s);
    }
}

