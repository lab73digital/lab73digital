    $('.js-slider-wrapper').bxSlider({
        mode: 'fade',
        pager: false
    });

    $('.main-page__top-slider').bxSlider({
        mode: 'fade',
        pager: false
    });
    $('.article__slider').bxSlider({
        mode: 'fade',
        adaptiveHeight: false,
        pager: false
    });
    $('.doc-consult__slider').bxSlider({
        mode: 'fade',
        pager: false
    });
    $('.readers-questions__slider').bxSlider({
        mode: 'fade',
        adaptiveHeight: true
    });
    $('.disease-medics__slider').bxSlider({
        mode: 'fade',
        adaptiveHeight: true,
        pager: false
    });
    if ($('.m__disease-medics__slider--inner').length) {
        $('.m__disease-medics__slider--inner').bxSlider({
            minSlides: 3,
            maxSlides: 4,
            captions: true,
            controls: false
        });
    }
    /* lekarstva scroll to h3 */
    if ($('.lekarstva-page .lekarstva-page__lekarstvo').length) {
        var lekarstvoDescrContainer = document.querySelector('.lekarstva-page__lekarstvo__tabs__subtabs__container');
        var lekarstvoDescrTrigger = document.querySelectorAll('.lekarstva-page__lekarstvo__tabs__subtabs__nav-item');
        var allHeadings = lekarstvoDescrContainer.querySelectorAll('h1,h2,h3,h4,h5,h6,h7');

        [].forEach.call(lekarstvoDescrTrigger, function (x, i) {
            lekarstvoDescrTrigger[i].addEventListener('click', function () {
                window.scroll(0, findPos(allHeadings[i]));
            }, false);
        });

        function findPos(obj) {
            var curtop = 0;
            if (obj.offsetParent) {
                do {
                    curtop += obj.offsetTop;
                } while (obj = obj.offsetParent);
                return [curtop];
            }
        }
    }

    if ($('.content-lists').length) {
        var lekarstvoDescrContainer = document.querySelector('.content-lists__container-content');
        var lekarstvoDescrTrigger = document.querySelectorAll('.content-lists__nav-item');
        var allHeadings = lekarstvoDescrContainer.querySelectorAll('h1,h2,h3,h4,h5,h6');

        [].forEach.call(lekarstvoDescrTrigger, function (x, i) {
            lekarstvoDescrTrigger[i].addEventListener('click', function () {
                window.scroll(0, findPos(allHeadings[i]));
            }, false);
        });

        function findPos(obj) {
            var curtop = 0;
            if (obj.offsetParent) {
                do {
                    curtop += obj.offsetTop;
                } while (obj = obj.offsetParent);
                return [curtop];
            }
        }
    }

    /**
     * TABS
     */
    $(document).on('click', '[data-tabs-nav]', function () {
        var $this = $(this);
        var $id = $this.data('tab-href');
        var $parent = $this.closest('[data-tabs-container]');
        console.log($id);

        if ($this.hasClass('active')) {
            return false;
        }

        $parent.children('[data-tabs-navigation]').children('[data-tabs-nav]').removeClass('active');
        $parent.children('[data-tab-id]').removeClass('active');
        $parent.find('.simptoms-block').children('[data-tab-id]').removeClass('active'); // NEED TO BE FIXED
        $this.addClass('active');
        $parent.find('[data-tab-id=' + $id + ']').addClass('active');
    });
    /**
     * Simptoms Human Points
     */
    if ($('[data-point-container]').length) {
        $(document).on('click', '[data-point]', function () {
            var $this = $(this);
            var $parent = $this.parents('[data-point-container]');
            var $id = $this.data('id');
            var $simptomsCont = $parent.find('.shown[data-point-block]');

            if ($this.hasClass('active')) return false;

            $.ajax({
                url: '/symptoms/bodyparts/',
                data: {bodyparts_id: $id},
                dataType: 'json',
                type: 'POST',
                success: function (data) {

                    $('.simptoms-block__container--popular__col').empty();

                    if (data.symptoms.length > 0) {

                        for (var i = 0; i < data.symptoms.length; i++) {
                            html = '<a href="/symptoms/' + data.symptoms[i].url + '/" class="name">' + data.symptoms[i].name + '</a>';
                            $('.simptoms-block__container--popular__col').append(html);
                        }

                        $('.simptoms-block__container--popular').addClass('active').addClass('shown').slideDown(300);
                    } else {

                    }

                }
            });

            if (!$parent.find('[data-simptoms-container]').hasClass('opened')) {
                $parent.find('[data-simptoms-container]').addClass('opened').slideDown(150);
            }

            $simptomsCont.css('display', 'none');
            $('.simptoms-block__container').find('.active').removeClass('active');
            $('[data-point-container]').find('.active').removeClass('active');
            $parent.find('.shown').removeClass('shown');

            $('[data-point="' + $id + '"]').addClass('active');
            $parent.find('[data-point-block="' + $id + '"]').addClass('active').addClass('shown').slideDown(300);
        });
        $(document).on('mouseenter', '[data-point]', function () {
            var $this = $(this);
            var $parent = $this.parents('[data-point-container]');
            var $id = $this.data('point');

            $parent.find('.hover').removeClass('hover');
            $parent.find('[data-point="' + $id + '"]').addClass('hover');
        });
        $(document).on('mouseleave', '[data-point]', function () {
            var $this = $(this);
            var $parent = $this.parents('[data-point-container]');
            var $id = $this.data('point');
            $parent.find('.hover').removeClass('hover');
        });
    }


    /**
     *   .
     *     action
     */
    $('.js-search-form input[type=search]').autocomplete({
        serviceUrl: $('.js-search-form').attr('action'),
        paramName: 'q',
        type: 'POST',
        onSelect: function (suggestion) {
            if (suggestion.url) {
                location.href = suggestion.url;
            }
        },
        onSearchComplete: function (query, suggestions) {
            $('.article-container__search-form__input').keydown(function(){
                if(event.keyCode == 13){
                    event.preventDefault();
                    if (suggestions.length !== 0) {
                        location.href = suggestions[0].url;
                    }
                }
            });
            $('.article-container__search-form__submit').click(function () {
                event.preventDefault();
                if (suggestions.length !== 0) {
                    location.href = suggestions[0].url;
                }
            });
        }
    });

    $('.js-more-items').on('click', function (e) {
        e.preventDefault();
        var offset = $(this).data('offset');
        var limit = $(this).data('limit');
        var type = $(this).data('type');
        $.ajax({
            url: $(this).data('action'),
            data: {type: type, offset: offset, limit: limit, where: $(this).data('where')},
            dataType: 'json',
            type: 'POST',
            success: function (data) {
                if (data.items.length > 0) {
                    for (var i = 0; i < data.items.length; i++) {
                        html = new EJS({url: '/ejs/more_' + type + '.ejs'}).render(data.items[i]);
                        $('.js-more-result').append(html);
                    }
                    $('.js-more-items').attr('data-offset', (offset + limit)).data('offset', (offset + limit));
                } else {
                    $('.js-more-result').append("<p>  </p>");
                    $('.js-more-items').hide();
                }

            }
        });
    });

    /* Consultacii page*/
