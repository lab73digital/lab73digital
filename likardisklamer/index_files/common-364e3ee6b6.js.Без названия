if (document.location.href.indexOf('/lekarstva/Viagra')==-1) {
    $(document).on('click', '.stars-rating__star', function (e) {
        var parent = $(this).parents('.stars-rating');
        var dataValue = $('.stars-rating__star').attr('data-value');
        if (!parent.is('.not-voted')) {
            return false;
        }
        else {
            parent.removeClass('not-voted');
            parent.find('.stars-rating__star').each(function () {
                $(this).removeClass('active');
                if ($(this).attr('data-value') < $(e.target).attr('data-value')) {
                    $(this).addClass('active');
                }
            });

            $.ajax({
                url: '/ratings/setrating/',
                data: {points: $(this).data('value'), object_id: $(this).data('object_id'), type: $(this).data('type')},
                dataType: 'json',
                type: 'POST',
                success: function (data) {
                    if (data.success === 1) {
                        //location.reload();
                    } else {
                        $('.rating-msg small').empty().show().text(data.error).fadeOut(3000);
                    }

                }
            });

            $(this).addClass('active');
        }
    });
}



function deleteAnswer(id) {
    $.ajax({
        url: '/consult/deleteanswer/',
        data: {id: id},
        dataType: 'json',
        type: 'POST',
        success: function (data) {
            if (data.success === 1) {
                location.reload();
            } else {

            }

        }
    });
}

function hasClass(a, c) {
    return 1 === a.nodeType && -1 < (" " + a.className + " ").replace(/[\n\t\r]/g, " ").indexOf(" " + c + " ") ? !0 : !1
}
(function () {
    var mobHeader = document.querySelector('.mobile__header');
    var menuTrigger = document.querySelector('.mobile__header-trigger');
    var sideMenu = document.querySelector('.mobile__side-menu');
    var overlay = document.querySelector('.mobile__overlay');
    var mobHeaderOverlay = document.querySelector('.mobile__header-trigger__overlay');
    var OPEN = 'is-visible';
    menuTrigger.addEventListener('click', function () {
        if (hasClass(menuTrigger, OPEN)) {
            mobHeader.classList.remove(OPEN);
            sideMenu.classList.remove(OPEN);
            overlay.classList.remove(OPEN);
            menuTrigger.classList.remove(OPEN);
        }
        else {
            mobHeader.classList.add(OPEN);
            sideMenu.classList.add(OPEN);
            overlay.classList.add(OPEN);
            menuTrigger.classList.add(OPEN);
        }
    });
    overlay.addEventListener('click', function () {
        mobHeader.classList.remove(OPEN);
        sideMenu.classList.remove(OPEN);
        overlay.classList.remove(OPEN);
        menuTrigger.classList.remove(OPEN);
    });
    mobHeaderOverlay.addEventListener('click', function () {
        mobHeader.classList.remove(OPEN);
        sideMenu.classList.remove(OPEN);
        overlay.classList.remove(OPEN);
        menuTrigger.classList.remove(OPEN);
    });

    /* Open&Close mobile search-form */
    var mSearchForm = document.querySelector('.mobile__header__search-form');
    var mSearchOpen = document.querySelector('.mobile__header__search-trigger');
    var mSearchClose = document.querySelector('.mobile__header__search-close');

    mSearchOpen.addEventListener('click', function (e) {
        e.preventDefault();
        mSearchForm.classList.add(OPEN);
    });
    mSearchClose.addEventListener('click', function (e) {
        e.preventDefault();
        mSearchForm.classList.remove(OPEN);
    });
})();

function formatRepo(repo) {
    if (repo.loading) return repo.text;

    var markup = "<div class='select2-result-repository clearfix'>" +
        "<div class='select2-result-repository__meta'>" +
        "<div class='select2-result-repository__title'>" + repo.title + "</div>";

    markup += "</div></div>";

    return markup;
}

function formatRepoSelection(repo) {
    if (repo.id > 0) {
        location.href = repo.url;
    }
    return repo.title;
}

/*
 * Animated scrollTop button
 */
document.querySelector('.scrollTop').onclick = function () {
    var e = document.documentElement;
    if (e.scrollTop === 0) {
        var t = e.scrollTop;
        ++e.scrollTop;
        e = t + 1 === e.scrollTop-- ? e : document.body;
    }
    scrollTo(e, 0, 500);
}

function scrollTo(element, to, duration) {
    var start = element.scrollTop,
        change = to - start,
        currentTime = 0,
        increment = 20;

    var animateScroll = function () {
        currentTime += increment;
        var val = easeInOutQuad(currentTime, start, change, duration);
        element.scrollTop = val;
        if (currentTime < duration) {
            setTimeout(animateScroll, increment);
        }
    };
    animateScroll();
}

//t = current time
//b = start value
//c = change in value
//d = duration
function easeInOutQuad(t, b, c, d) {
    t /= d / 2;
    if (t < 1) return c / 2 * t * t + b;
    t--;
    return -c / 2 * (t * (t - 2) - 1) + b;
};

(function () {
    /*
     * Registration popup
     */
    var registerTrig = document.querySelectorAll('[data-popup-action]');
    var popupSense;

    [].forEach.call(registerTrig, function (x, i) {
        registerTrig[i].addEventListener('click', function (event) {
            event.preventDefault();

            var action = this.getAttribute('data-popup-action');
            var name = this.getAttribute('data-popup-name');

            if (action == 'open') {
                // open popup and overlay
                document.querySelector('[data-popup-popup="' + name + '"]').classList.add('opened');
                document.querySelector('[data-popup-overlay="' + name + '"]').classList.add('opened');
                // set popupSense
                popupSense = this.getAttribute('data-popup-sense-href');
                // hide current tab and open target one
                var tabs = document.querySelectorAll('[data-popup-sense-id]');
                [].forEach.call(tabs, function (x, i) {
                    if (hasClass(tabs[i], 'inner-active')) tabs[i].classList.remove('inner-active');
                });
                document.querySelector('[data-popup-sense-id="' + popupSense + '"]').classList.add('inner-active');
            } else {
                // close popup and overlay
                document.querySelector('[data-popup-popup="' + name + '"]').classList.remove('opened');
                document.querySelector('[data-popup-overlay="' + name + '"]').classList.remove('opened');
                //hide all tabs inside closed popup
                document.querySelector('[data-popup-sense-id="' + popupSense + '"]').classList.remove('inner-active');
            }
        }, false);
    });
})();
(function () {
    var forms = document.querySelectorAll('.enter-registr-recov__form');

    [].forEach.call(forms, function (x, i) {
        var _action = forms[i].getAttribute('action');

        forms[i].addEventListener('submit', function () {
            submitFormAjax(forms[i], _action);
        }, false);
    });
})();
function submitFormAjax(formEl, act) {
    event.preventDefault();
    var _action = act;
    var elements = formEl.getElementsByTagName('input');
    var formData = new FormData();

    for (var i = 0; i < elements.length; i++) {
        formData.append(elements[i].name, elements[i].value);
    }

    var xhr = new XMLHttpRequest();
    var url = _action;
    if (document.referrer) {
        var referrerUrl = document.referrer.split('/')[2];
        referrerUrl = referrerUrl.split('.')[0];
    } else {
        referrerUrl = false;
    }
    xhr.open("POST", url, true);
    xhr.send(formData);

    xhr.onreadystatechange = function () {
        var data = $.parseJSON(xhr.responseText);

        if (xhr.readyState == 4 && xhr.status == 200) {
            if (data.success == 1) {
                if (referrerUrl == 'forum') {
                    window.location.replace(document.referrer);
                } else {
                    location.reload();
                }
            }
        }
        if (xhr.readyState == 4 && xhr.status == 400&& data.error == 0) {
            $('.js-auth-form-error').empty()
            $.each(data.msg, function (key, value) {
                $('.js-auth-form-error').text(value);
            });
        }
        if (xhr.readyState == 4 && xhr.status == 400 && data.error == 1) {
            $('.js-auth-form-error-login').empty()
            $.each(data.msg, function (key, value) {
                $('.js-auth-form-error-login').text(value);
            });
        }
    }
}

$('.js-menu-hover').hover(function () {
    $(this).children('ul').stop(true, false, true).slideToggle(500);
});


$('.js-mobile-menu-toggle').on('click', function () {
    $('.js-mobile-menu').slideToggle(500);
});

$(document).ready(function () {

    $('#gazetaConfirm').show();
    $('.close-popup').on('click', function () {
        $('#gazetaConfirm').hide();
    });

    $('.article_add_links').click(function (e) {
        e.preventDefault();
        var offset = $(this).data('offset');
        var limit = $(this).data('limit');
        var type = $(this).data('type');
        var id = $(this).data('id');
        $.ajax({
            url: $(this).data('action'),
            data: {type: type, offset: offset, limit: limit, id: id},
            dataType: 'json',
            type: 'POST',
            success: function (data) {
                if (data.items.length > 0) {
                    for (var i = 0; i < data.items.length; i++) {
                        html = new EJS({url: '/ejs/more_' + type + '.ejs'}).render(data.items[i]);
                        $('.additional_cycle').append(html);
                    }
                    $('.article_add_links').attr('data-offset', (offset + limit)).data('offset', (offset + limit));
                } else {
                    $('.article_add_links').fadeOut(300);
                }

            }
        });
    });

    $('ul.tabs').delegate('li:not(.current)', 'click', function () {
        $(this).addClass('current').siblings().removeClass('current')
            .parents('div.section').find('div.box').hide().eq($(this).index()).fadeIn(150);
    })
    setTimeout(qwee, 2000);

    if ($('.content-lists__container-content').children().first().hasClass('attach-image')) {
        $('.content-lists__container-content').children().first().after($('.content-lists-block'))
    }
});
function qwee() {
    if (!($('.gsc-cursor .prev').length || $('.gsc-cursor .next').length) == 0) {
        return;
    }
    $('.gsc-cursor').prepend('<span class="prev"></span>');
    $('.gsc-cursor').append('<span class="next"></span>');
    if (!$('.gsc-cursor-page.gsc-cursor-current-page').prev('.gsc-cursor-page').length) {
        $('.gsc-cursor .prev').css('opacity', '0.5');
    }
    if (!$('.gsc-cursor-page.gsc-cursor-current-page').next('.gsc-cursor-page').length) {
        $('.gsc-cursor .next').css('opacity', '0.5');
    }
    $('.gsc-cursor .prev').on('click', function () {
        if ($('.gsc-cursor-page.gsc-cursor-current-page').prev('.gsc-cursor-page').length) {
            $('.gsc-cursor-page.gsc-cursor-current-page').prev().click();
        }
        setTimeout(qwee, 1000);
    })
    $('.gsc-cursor .next').on('click', function () {
        if ($('.gsc-cursor-page.gsc-cursor-current-page').next('.gsc-cursor-page').length) {
            $('.gsc-cursor-page.gsc-cursor-current-page').next().click();
        }
        setTimeout(qwee, 1000);
    })
    $('.gsc-cursor-page').on('click', function () {
        setTimeout(qwee, 1000);
    })
}

$(document).ready(function () {
    $(".doc-slider-item h4.name").each(function (i) {
        len = $(this).text().length;
        if (len > 40) {
            $(this).text($(this).text().substr(0, 37) + '...');
        }
    });
    $(".consultacii-page__list .consultacii-page__list-item__question").each(function (i) {
        len = $(this).text().length;
        if (len > 173) {
            $(this).text($(this).text().substr(0, 170) + '...');
        }
    });
});



$(document).ready(function () {
    if ($('.js-more-items-scroll').length) {
        var timer;

        $(window).scroll(function () {

            sct = $(window).scrollTop();

            if (timer) {
                window.clearTimeout(timer);
            }
            timer = window.setTimeout(function () {
                if (!($(window).scrollTop() + $(window).height() >= $(document).height() - $('.footer--main').height())) {
                    if ($(window).scrollTop() + $(window).height() >= $(document).height() - $('.additional-info--main').height() - $('.footer--main').height() - 200) {
                        var $this = $('.js-more-items-scroll');
                        var offset = $('.js-more-items-scroll').data('offset');
                        var limit = $this.data('limit');
                        var type = $this.data('type');
                        $.ajax({
                            url: $this.data('action'),
                            data: {type: type, offset: offset, limit: limit, where: $this.data('where')},
                            dataType: 'json',
                            type: 'POST',
                            success: function (data) {
                                if (data.items.length > 0) {
                                    for (var i = 0; i < data.items.length; i++) {
                                        html = new EJS({url: '/ejs/more_' + type + '.ejs'}).render(data.items[i]);
                                        $('.js-more-result').append(html);
                                    }
                                    $('.js-more-items-scroll').attr('data-offset', (offset + limit)).data('offset', (offset + limit));
                                } else {
                                    $('.js-more-result').append("<p>  </p>");
                                }

                            }
                        });
                    }
                }
            }, 100);

        });
    }

    //$(".sticky").sticky({topSpacing:0, bottomSpacing: footerHeight + 20});
});


(function ($) {
    var sticky_top;
    var sticky_left;
    var footer_top;
    var sct;
    var sticked=false;

    $(function () {
        if ($(".sticky").length>0) {
            sticky_top = $(".sticky").offset().top;
            sticky_left = $(".sticky").offset().left;
            footer_top = $('.footer--main').offset().top;
            sct = $(window).scrollTop();
            sticked=false;
        }
    });

    $(window).load(function () {
        if ($(".sticky").length>0) sticky_top = $(".sticky").offset().top;
    });

    $(window).resize(function () {
        if ((sct>=sticky_top)/* && (sct<=footer_top-20)*/) {
            $(".sticky").css({'position': 'static'});
            if ($(".sticky").length>0) sticky_left = $(".sticky").offset().left;
            $(".sticky").css({
                'position': 'fixed',
                'top': '0px',
                'left': sticky_left
            });
        }
    });

    $(window).scroll(function () {
        if ($(".sticky").length>0) {
            sct = $(window).scrollTop();

            if (!sticked) {
                sticky_top = $(".sticky").offset().top;
                sticky_left = $(".sticky").offset().left;
            }

            if ((sct>=sticky_top)/* && (sct<=footer_top-20)*/) {
                $(".sticky").css({
                    'position': 'fixed',
                    'top': '0px',
                    'left': sticky_left
                });

                sticked=true;
            }
            else {
                $(".sticky").css({'position': 'static'});
                sticked=false;
            }
        }
    });

})(jQuery);



