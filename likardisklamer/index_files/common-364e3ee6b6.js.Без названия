function adMixer() {
    var w = window, d = document;
    w.admixZArr = (w.admixZArr || []);
    w.admixerSmOptions = (w.admixerSmOptions || {});
    w.admixerSmOptions.showAdsOnLoad = true;

    window.amSlots = (window.amSlots || []); window.amSlots.push(
        {
            z: '9c657611-8c48-4b79-9500-4f85294e7f83',
            ph: 'admixer_9c6576118c484b7995004f85294e7f83_zone_6210_sect_1450_site_1383_rnd_1340113936',
            i: 'inv-nets'
        }
    );
}

